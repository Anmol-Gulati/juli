{
  "name": "Juli",
  "version": "1.3.1",
  "main": "public/electron.js",
  "scripts": {
    "template:install": "cd template && yarn install",
    "template:build": "cd template && yarn build && yarn install --production --no-bin-links --no-lockfile",
    "template:lint": "cd template && yarn lint",
    "template:electron-dev": "cd template && yarn electron-dev",
    "postinstall": "electron-builder install-app-deps && node hacky.js && yarn template:install",
    "build": "yarn template:build && react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint ./dist.js ./hacky.js ./public ./src --ext js && yarn template:lint",
    "start": "react-scripts start",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "dist": "yarn build && node dist.js",
    "test": "mocha ./tests"
  },
  "repository": "https://github.com/quanglam2807/juli",
  "author": "Quang Lam <quang.lam2807@gmail.com>",
  "license": "MPL-2.0",
  "dependencies": {
    "electron-is-dev": "0.3.0",
    "electron-packager": "11.1.0",
    "electron-settings": "3.1.4",
    "electron-window-state": "4.1.1",
    "file-type": "7.6.0",
    "follow-redirects": "1.4.1",
    "fs-extra": "5.0.0",
    "icon-gen": "1.2.0",
    "is-url": "1.2.2",
    "jimp": "0.2.28",
    "read-chunk": "2.1.0",
    "tmp": "0.0.33",
    "windows-shortcuts": "0.1.6",
    "yargs-parser": "9.0.2"
  },
  "devDependencies": {
    "algoliasearch": "3.25.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "blueimp-md5": "2.10.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "classnames": "2.2.5",
    "concurrently": "3.5.1",
    "cross-env": "5.1.4",
    "electron": "1.8.4",
    "electron-builder": "20.5.1",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "material-ui": "1.0.0-beta.37",
    "material-ui-icons": "1.0.0-beta.36",
    "mocha": "5.0.4",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "react-scripts": "1.0.17",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "replace-in-file": "3.2.0",
    "request": "2.85.0",
    "semver": "5.5.0",
    "spectron": "3.8.0",
    "typeface-roboto": "0.0.54",
    "wait-on": "2.1.0"
  },
  "homepage": "./",
  "private": true
}
